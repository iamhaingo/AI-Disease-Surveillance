n","    # Smooth the specified columns\n","    smoothed_df = df.copy()\n","    for column in columns:\n","        if column in smoothed_df.columns:\n","            smoothed_df[column] = smooth_data(smoothed_df[column], window_size)\n","\n","    # Plot the smoothed data if a plot function is provided\n","    if plot_function is not None:\n","        plot_function(smoothed_df, columns)\n","\n","    return smoothed_df"],"metadata":{"id":"znfddXdNVA8B","executionInfo":{"status":"ok","timestamp":1718839112542,"user_tz":-120,"elapsed":1082,"user":{"displayName":"Ny Haingo Miantsatiana ANDRY","userId":"02576837319316170548"}},"cellView":"form"},"execution_count":1002,"outputs":[]},{"cell_type":"code","source":["#@title sorting\n","smoothed_count_df_south_africa = smoothed_count_df_south_africa.sort_values(by='date')"],"metadata":{"id":"rDeUWJp3WXhT","executionInfo":{"status":"ok","timestamp":1718839112542,"user_tz":-120,"elapsed":14,"user":{"displayName":"Ny Haingo Miantsatiana ANDRY","userId":"02576837319316170548"}}},"execution_count":1003,"outputs":[]},{"cell_type":"code","source":["#@title annomaly\n","import torch\n","import torch.nn as nn\n","import torch.optim as optim\n","import pandas as pd\n","import matplotlib.pyplot as plt\n","import numpy as np\n","\n","# Assuming smoothed_count_df_south_africa and target_south_africa are already defined\n","# Convert data to tensor\n","data_tensor = torch.tensor(smoothed_count_df_south_africa[target_south_africa].values, dtype=torch.float32).unsqueeze(0)\n","\n","# Define the LSTM Autoencoder model\n","class LSTMAutoencoder(nn.Module):\n","    def __init__(self, input_size, hidden_size, num_layers=1):\n","        super(LSTMAutoencoder, self).__init__()\n","        self.encoder = nn.LSTM(input_size, hidden_size, num_layers, batch_first=True)\n","        self.decoder = nn.LSTM(hidden_size, input_size, num_layers, batch_first=True)\n","\n","    def forward(self, x):\n","        _, (hidden, _) = self.encoder(x)\n","        hidden = hidden.repeat(x.size(1), 1, 1).permute(1, 0, 2)\n","        reconstructed, _ = self.decoder(hidden)\n","        return reconstructed\n","\n","# Hyperparameters\n","input_size = data_tensor.size(2)\n","hidden_size = 10\n","learning_rate = 0.01\n","num_epochs = 100\n","\n","# Initialize model, loss function, and optimizer\n","model_x = LSTMAutoencoder(input_size, hidden_size)\n","criterion = nn.MSELoss()\n","optimizer = optim.Adam(model_x.parameters(), lr=learning_rate)\n","\n","# Train the model\n","losses = []\n","for epoch in range(num_epochs):\n","    outputs = model_x(data_tensor)\n","    loss = criterion(outputs, data_tensor)\n","    optimizer.zero_grad()\n","    loss.backward()\n","    optimizer.step()\n","    losses.append(loss.item())\n","    if (epoch + 1) % 10 == 0:\n","        print(f'Epoch [{epoch+1}/{num_epochs}], Loss: {loss.item():.4f}')\n","\n","# Plot training loss curve\n","plt.plot(losses)\n","plt.xlabel('Epoch')\n","plt.ylabel('Loss')\n","plt.title('Training Loss Curve')\n","plt.show()\n","\n","# Evaluate reconstruction errors\n","with torch.no_grad():\n","    reconstructed = model_x(data_tensor)\n","    errors = torch.mean((data_tensor - reconstructed)**2, dim=2).numpy().flatten()\n","\n","# Plot reconstruction errors with dates\n","plt.plot(smoothed_count_df_south_africa['date'], errors, linestyle='-', color='c', label='Reconstruction Error')\n","plt.xlabel('Date')\n","plt.ylabel('Reconstruction Error')\n","plt.title('Reconstruction Error for Anomaly Detection')\n","plt.xticks(rotation=45)\n","plt.legend()\n","plt.show()\n","\n","# Detect spikes\n","spike_indices = np.where(errors > np.mean(errors) + 2 * np.std(errors))[0]\n","dates_with_spikes = smoothed_count_df_south_africa['date'].iloc[spike_indices]\n","print(\"Indices of spikes:\", spike_indices)\n","print(\"Dates corresponding to spike indices:\")\n","print(dates_with_spikes)"],"metadata":{"id":"IRh0KMzlYqbY","colab":{"base_uri":"https://localhost:8080/","height":1000},"executionInfo":{"status":"ok","timestamp":1718680296056,"user_tz":-120,"elapsed":2613,"user":{"displayName":"Ny Haingo Miantsatiana ANDRY","userId":"02576837319316170548"}},"outputId":"91294f19-3bdb-48b0-de1f-3c9fa531368d","cellView":"form"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Epoch [10/100], Loss: 6.9448\n","Epoch [20/100], Loss: 5.8241\n","Epoch [30/100], Loss: 4.8451\n","Epoch [40/100], Loss: 4.4393\n","Epoch [50/100], Loss: 4.3712\n","Epoch [60/100], Loss: 4.3554\n","Epoch [70/100], Loss: 4.3497\n","Epoch [80/100], Loss: 4.3469\n","Epoch [90/100], Loss: 4.3452\n","Epoch [100/100], Loss: 4.3440\n"]},{"output_type":"display_data","data":{"text/plain":["<Figure size 640x480 with 1 Axes>"],"image/png":"\n"},"metadata":{}},{"output_type":"display_data","data":{"text/plain":["<Figure size 640x480 with 1 Axes>"],"image/png":"\n"},"metadata":{}},{"output_type":"stream","name":"stdout","text":["Indices of spikes: [31 32 60 61 62 63 64 65 78 79 80 81]\n","Dates corresponding to spike indices:\n","185    2020-01-24\n","184    2020-01-25\n","156    2020-02-26\n","155    2020-02-28\n","154    2020-03-01\n","153    2020-03-02\n","152    2020-03-03\n","151    2020-03-04\n","138    2020-03-17\n","137    2020-03-18\n","136    2020-03-19\n","135    2020-03-20\n","Name: date, dtype: object\n"]}]},{"cell_type":"code","source":["#@title early detection\n","import torch\n","import torch.nn as nn\n","import torch.optim as optim\n","import pandas as pd\n","import matplotlib.pyplot as plt\n","import numpy as np\n","\n","# Assuming smoothed_count_df_south_africa and target_south_africa are already defined\n","# Convert data to tensor\n","truncated = smoothed_count_df_south_africa.head(22)\n","data_tensor = torch.tensor(truncated[target_south_africa].values, dtype=torch.float32).unsqueeze(0)\n","\n","# Define the LSTM Autoencoder model\n","class LSTMAutoencoder(nn.Module):\n","    def __init__(self, input_size, hidden_size, num_layers=1):\n","        super(LSTMAutoencoder, self).__init__()\n","        self.encoder = nn.LSTM(input_size, hidden_size, num_layers, batch_first=True)\n","        self.decoder = nn.LSTM(hidden_size, input_size, num_layers, batch_first=True)\n","\n","    def forward(self, x):\n","        _, (hidden, _) = self.encoder(x)\n","        hidden = hidden.repeat(x.size(1), 1, 1).permute(1, 0, 2)\n","        reconstructed, _ = self.decoder(hidden)\n","        return reconstructed\n","\n","# Hyperparameters\n","input_size = data_tensor.size(2)\n","hidden_size = 10\n","learning_rate = 0.01\n","num_epochs = 100\n","\n","# Initialize model, loss function, and optimizer\n","model_x = LSTMAutoencoder(input_size, hidden_size)\n","criterion = nn.MSELoss()\n","optimizer = optim.Adam(model_x.parameters(), lr=learning_rate)\n","\n","# Train the model\n","losses = []\n","for epoch in range(num_epochs):\n","    outputs = model_x(data_tensor)\n","    loss = criterion(outputs, data_tensor)\n","    optimizer.zero_grad()\n","    loss.backward()\n","    optimizer.step()\n","    losses.append(loss.item())\n","    if (epoch + 1) % 10 == 0:\n","        print(f'Epoch [{epoch+1}/{num_epochs}], Loss: {loss.item():.4f}')\n","\n","# Plot training loss curve\n","plt.plot(losses)\n","plt.xlabel('Epoch')\n","plt.ylabel('Loss')\n","plt.title('Training Loss Curve')\n","plt.show()\n","\n","# Evaluate reconstruction errors\n","with torch.no_grad():\n","    reconstructed = model_x(data_tensor)\n","    errors = torch.mean((data_tensor - reconstructed)**2, dim=2).numpy().flatten()\n","\n","# Plot reconstruction errors with dates\n","plt.plot(truncated['date'], errors, linestyle='-', color='c', label='Reconstruction Error')\n","plt.xlabel('Date')\n","plt.ylabel('Reconstruction Error')\n","plt.title('Reconstruction Error for Anomaly Detection')\n","plt.xticks(rotation=45)\n","plt.legend()\n","plt.show()\n","\n","# Detect spikes\n","spike_indices = np.where(errors > np.mean(errors) + 2 * np.std(errors))[0]\n","dates_with_spikes = truncated['date'].iloc[spike_indices]\n","print(\"Indices of spikes:\", spike_indices)\n","print(\"Dates corresponding to spike indices:\")\n","print(dates_with_spikes)"],"metadata":{"id":"za7B4WEqCstS","colab":{"base_uri":"https://localhost:8080/","height":1000},"executionInfo":{"status":"ok","timestamp":1718839124004,"user_tz":-120,"elapsed":2261,"user":{"displayName":"Ny Haingo Miantsatiana ANDRY","userId":"02576837319316170548"}},"outputId":"30216c27-89fb-4521-c9fe-1344bf933e76","cellView":"form"},"execution_count":1004,"outputs":[{"output_type":"stream","name":"stdout","text":["Epoch [10/100], Loss: 0.1890\n","Epoch [20/100], Loss: 0.0866\n","Epoch [30/100], Loss: 0.0878\n","Epoch [40/100], Loss: 0.0855\n","Epoch [50/100], Loss: 0.0828\n","Epoch [60/100], Loss: 0.0821\n","Epoch [70/100], Loss: 0.0817\n","Epoch [80/100], Loss: 0.0811\n","Epoch [90/100], Loss: 0.0806\n","Epoch [100/100], Loss: 0.0802\n"]},{"output_type":"display_data","data":{"text/plain":["<Figure size 640x480 with 1 Axes>"],"image/png":"\n"},"metadata":{}},{"output_type":"display_data","data":{"text/plain":["<Figure size 640x480 with 1 Axes>"],"image/png":"\n"},"metadata":{}},{"output_type":"stream","name":"stdout","text":["Indices of spikes: [21]\n","Dates corresponding to spike indices:\n","195    2019-12-13\n","Name: date, dtype: object\n"]}]},{"cell_type":"code","source":["#@title south africa"],"metadata":{"id":"3R9uc1Nmw4S8"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["#@title annomaly\n","import torch\n","import torch.nn as nn\n","import torch.optim as optim\n","import pandas as pd\n","import matplotlib.pyplot as plt\n","import numpy as np\n","\n","# Assuming smoothed_count_df_south_africa and target_south_africa are already defined\n","# Convert data to tensor\n","data_tensor = torch.tensor(smoothed_count_df_south_africa[target_south_africa].values, dtype=torch.float32).unsqueeze(0)\n","\n","# Define the LSTM Autoencoder model\n","class LSTMAutoencoder(nn.Module):\n","    def __init__(self, input_size, hidden_size, num_layers=1):\n","        super(LSTMAutoencoder, self).__init__()\n","        self.encoder = nn.LSTM(input_size, hidden_size, num_layers, batch_first=True)\n","        self.decoder = nn.LSTM(hidden_size, input_size, num_layers, batch_first=True)\n","\n","    def forward(self, x):\n","        _, (hidden, _) = self.encoder(x)\n","        hidden = hidden.repeat(x.size(1), 1, 1).permute(1, 0, 2)\n","        reconstructed, _ = self.decoder(hidden)\n","        return reconstructed\n","\n","# Hyperparameters\n","input_size = data_tensor.size(2)\n","hidden_size = 10\n","learning_rate = 0.01\n","num_epochs = 100\n","\n","# Initialize model, loss function, and optimizer\n","model_x = LSTMAutoencoder(input_size, hidden_size)\n","criterion = nn.MSELoss()\n","optimizer = optim.Adam(model_x.parameters(), lr=learning_rate)\n","\n","# Train the model\n","losses = []\n","for epoch in range(num_epochs):\n","    outputs = model_x(data_tensor)\n","    loss = criterion(outputs, data_tensor)\n","    optimizer.zero_grad()\n","    loss.backward()\n","    optimizer.step()\n","    losses.append(loss.item())\n","    if (epoch + 1) % 10 == 0:\n","        print(f'Epoch [{epoch+1}/{num_epochs}], Loss: {loss.item():.4f}')\n","\n","# Plot training loss curve\n","plt.plot(losses)\n","plt.xlabel('Epoch')\n","plt.ylabel('Loss')\n","plt.title('Training Loss Curve')\n","plt.show()\n","\n","# Evaluate reconstruction errors\n","with torch.no_grad():\n","    reconstructed = model_x(data_tensor)\n","    errors = torch.mean((data_tensor - reconstructed)**2, dim=2).numpy().flatten()\n","\n","# Plot reconstruction errors with dates\n","plt.plot(smoothed_count_df_south_africa['date'], errors, linestyle='-', color='c', label='Reconstruction Error')\n","plt.xlabel('Date')\n","plt.ylabel('Reconstruction Error')\n","plt.title('Reconstruction Error for Anomaly Detection')\n","plt.xticks(rotation=45)\n","plt.legend()\n","plt.show()\n","\n","# Detect spikes\n","spike_indices = np.where(errors > np.mean(errors) + 2 * np.std(errors))[0]\n","dates_with_spikes = smoothed_count_df_south_africa['date'].iloc[spike_indices]\n","print(\"Indices of spikes:\", spike_indices)\n","print(\"Dates corresponding to spike indices:\")\n","print(dates_with_spikes)"],"metadata":{"cellView":"form","id":"TDaKB82fxKoJ","executionInfo":{"status":"ok","timestamp":1718680309962,"user_tz":-120,"elapsed":1653,"user":{"displayName":"Ny Haingo Miantsatiana ANDRY","userId":"02576837319316170548"}},"outputId":"8e374478-d02b-48a7-da51-9fadc9717999","colab":{"base_uri":"https://localhost:8080/","height":1000}},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Epoch [10/100], Loss: 6.6450\n","Epoch [20/100], Loss: 5.6690\n","Epoch [30/100], Loss: 5.0179\n","Epoch [40/100], Loss: 4.4422\n","Epoch [50/100], Loss: 4.3734\n","Epoch [60/100], Loss: 4.3584\n","Epoch [70/100], Loss: 4.3528\n","Epoch [80/100], Loss: 4.3499\n","Epoch [90/100], Loss: 4.3481\n","Epoch [100/100], Loss: 4.3468\n"]},{"output_type":"display_data","data":{"text/plain":["<Figure size 640x480 with 1 Axes>"],"image/png":"\n"},"metadata":{}},{"output_type":"display_data","data":{"text/plain":["<Figure size 640x480 with 1 Axes>"],"image/png":"\n"},"metadata":{}},{"output_type":"stream","name":"stdout","text":["Indices of spikes: [31 32 60 61 62 63 64 65 78 79 80 81]\n","Dates corresponding to spike indices:\n","185    2020-01-24\n","184    2020-01-25\n","156    2020-02-26\n","155    2020-02-28\n","154    2020-03-01\n","153    2020-03-02\n","152    2020-03-03\n","151    2020-03-04\n","138    2020-03-17\n","137    2020-03-18\n","136    2020-03-19\n","135    2020-03-20\n","Name: date, dtype: object\n"]}]},{"cell_type":"code","source":["#@title early detection\n","import torch\n","import torch.nn as nn\n","import torch.optim as optim\n","import pandas as pd\n","import matplotlib.pyplot as plt\n","import numpy as np\n","\n","# Assuming smoothed_count_df_south_africa and target_south_africa are already defined\n","# Convert data to tensor\n","truncated = smoothed_count_df_south_africa.head(50)\n","data_tensor = torch.tensor(truncated[target_south_africa].values, dtype=torch.float32).unsqueeze(0)\n","\n","# Define the LSTM Autoencoder model\n","class LSTMAutoencoder(nn.Module):\n","    def __init__(self, input_size, hidden_size, num_layers=1):\n","        super(LSTMAutoencoder, self).__init__()\n","        self.encoder = nn.LSTM(input_size, hidden_size, num_layers, batch_first=True)\n","        self.decoder = nn.LSTM(hidden_size, input_size, num_layers, batch_first=True)\n","\n","    def forward(self, x):\n","        _, (hidden, _) = self.encoder(x)\n","        hidden = hidden.repeat(x.size(1), 1, 1).permute(1, 0, 2)\n","        reconstructed, _ = self.decoder(hidden)\n","        return reconstructed\n","\n","# Hyperparameters\n","input_size = data_tensor.size(2)\n","hidden_size = 10\n","learning_rate = 0.01\n","num_epochs = 100\n","\n","# Initialize model, loss function, and optimizer\n","model_x = LSTMAutoencoder(input_size, hidden_size)\n","criterion = nn.MSELoss()\n","optimizer = optim.Adam(model_x.parameters(), lr=learning_rate)\n","\n","# Train the model\n","losses = []\n","for epoch in range(num_epochs):\n","    outputs = model_x(data_tensor)\n","    loss = criterion(outputs, data_tensor)\n","    optimizer.zero_grad()\n","    loss.backward()\n","    optimizer.step()\n","    losses.append(loss.item())\n","    if (epoch + 1) % 10 == 0:\n","        print(f'Epoch [{epoch+1}/{num_epochs}], Loss: {loss.item():.4f}')\n","\n","# Plot training loss curve\n","plt.plot(losses)\n","plt.xlabel('Epoch')\n","plt.ylabel('Loss')\n","plt.title('Training Loss Curve')\n","plt.show()\n","\n","# Evaluate reconstruction errors\n","with torch.no_grad():\n","    reconstructed = model_x(data_tensor)\n","    errors = torch.mean((data_tensor - reconstructed)**2, dim=2).numpy().flatten()\n","\n","# Plot reconstruction errors with dates\n","plt.plot(truncated['date'], errors, linestyle='-', color='c', label='Reconstruction Error')\n","plt.xlabel('Date')\n","plt.ylabel('Reconstruction Error')\n","plt.title('Reconstruction Error for Anomaly Detection')\n","plt.xticks(rotation=45)\n","plt.legend()\n","plt.show()\n","\n","# Detect spikes\n","spike_indices = np.where(errors > np.mean(errors) + 2 * np.std(errors))[0]\n","dates_with_spikes = truncated['date'].iloc[spike_indices]\n","print(\"Indices of spikes:\", spike_indices)\n","print(\"Dates corresponding to spike indices:\")\n","print(dates_with_spikes)"],"metadata":{"cellView":"form","id":"jP6RmVMOw7eB","executionInfo":{"status":"ok","timestamp":1718680311825,"user_tz":-120,"elapsed":1869,"user":{"displayName":"Ny Haingo Miantsatiana ANDRY","userId":"02576837319316170548"}},"outputId":"7daf70cb-3e7c-4724-f58a-73fcd9da6e59","colab":{"base_uri":"https://localhost:8080/","height":1000}},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Epoch [10/100], Loss: 4.3594\n","Epoch [20/100], Loss: 3.6233\n","Epoch [30/100], Loss: 3.2844\n","Epoch [40/100], Loss: 3.1751\n","Epoch [50/100], Loss: 3.1554\n","Epoch [60/100], Loss: 3.1497\n","Epoch [70/100], Loss: 3.1467\n","Epoch [80/100], Loss: 3.1442\n","Epoch [90/100], Loss: 3.1409\n","Epoch [100/100], Loss: 3.1320\n"]},{"output_type":"display_data","data":{"text/plain":["<Figure size 640x480 with 1 Axes>"],"image/png":"\n"},"metadata":{}},{"output_type":"display_data","data":{"text/plain":["<Figure size 640x480 with 1 Axes>"],"image/png":"\n"},"metadata":{}},{"output_type":"stream","name":"stdout","text":["Indices of spikes: [30 31 32 33]\n","Dates corresponding to spike indices:\n","186    2020-01-23\n","185    2020-01-24\n","184    2020-01-25\n","183    2020-01-26\n","Name: date, dtype: object\n"]}]}]}