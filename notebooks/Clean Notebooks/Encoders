lists\n","    true_labels_flat = [label for sublist in true_labels for label in sublist]\n","    pred_labels_flat = [label for sublist in pred_labels for label in sublist]\n","\n","    # Compute the confusion matrix\n","    cm = confusion_matrix(true_labels_flat, pred_labels_flat, labels=label_list, normalize=normalize)\n","\n","    # Plot the confusion matrix\n","    fig, ax = plt.subplots(figsize=(10, 8))\n","    disp = ConfusionMatrixDisplay(confusion_matrix=cm, display_labels=label_list)\n","    disp.plot(ax=ax, cmap=plt.cm.Blues, values_format='.2f')\n","\n","    plt.title('Confusion Matrix')\n","    plt.show()\n","\n","# Example usage:\n","# Assuming true_predictions and true_labels are already defined and label_list contains the list of labels\n","plot_confusion_matrix(true_labels=true_labels, pred_labels=true_predictions, label_list=class_names)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":718},"id":"WjK99JxsfE54","executionInfo":{"status":"ok","timestamp":1718533721390,"user_tz":-120,"elapsed":98,"user":{"displayName":"Ny Haingo Miantsatiana ANDRY","userId":"02576837319316170548"}},"outputId":"8cb39708-48ce-455f-d4db-c24ac2e9300e"},"execution_count":null,"outputs":[{"output_type":"display_data","data":{"text/plain":["<Figure size 1000x800 with 2 Axes>"],"image/png":"\n"},"metadata":{}}]},{"cell_type":"code","source":["#@title import, news\n","%%capture\n","!pip install gnews newspaper3k\n","from gnews import GNews\n","from tqdm.notebook import tqdm_notebook as tqdm\n","from datetime import datetime, timedelta\n","import spacy\n","import re\n","import time\n","import pandas as pd\n","from google.colab import files"],"metadata":{"id":"8REf8kVhh1dM"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["def save_and_download_csv(df, keyword):\n","    df.to_csv(f\"{keyword}.csv\", index=False)\n","    files.download(f\"{keyword}.csv\")"],"metadata":{"id":"Nja8U82nzYdD"},"execution_count":null,"outputs":[]},{"cell_type":"mar